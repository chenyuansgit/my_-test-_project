<!DOCTYPE html>
<html lang="zh-CN">
<head>
    <%include ../../common/head.ejs%>
    <link href="<%=baseUrl%>/css/page/private/resume.css" rel="stylesheet"/>
    <!--[if lte IE 8]>
    <link href="<%=baseUrl%>/css/page/cssie/css-ie8-style.css" rel="stylesheet"/>
    <![endif]-->
    <!--[if lte IE 7]>
    <link href="<%=baseUrl%>/css/page/cssie/ie7fix.css" rel="stylesheet"/>
    <![endif]-->
</head>
<body>
<div class="header">
    <%include ../../common/header.ejs%>
    <div class="header-nav">
        <div class="header-nav-wrap">
            <a class="logo" href="/"></a>
            <ul class="header-link clearfix">
                <li><a class="header-link-item" href="/">首页</a></li>
                <li><a class="header-link-item" href="/j/search">实习招聘</a></li>
                <li><a class="header-link-item" href="/j/search?type=campus">校园招聘</a></li>
                <li>
                    <a class="header-link-item" href="/quickRecruit">快招</a><i class="icon-test"></i>
                    <ul class="sub-nav sub-nav-quickRecruit">
                        <li><a href="/quickRecruit/manage">我的快招</a></li>
                    </ul>
                </li>
                <li><a class="header-link-item" href="/activity">活动</a></li>
                <li><a class="header-link-item" href="/nest">求职头条</a></li>
            </ul>
        </div>
    </div>
</div>
<!--我的简历中部-->
<div class="container">
    <%var integrity = 10;%>
    <%if(resume){
    for(var i=0;i<1;i++){
        integrity = 0;
    %>
    <div class="myresume clearfix mb20" data-id="<%=resume[i].rid%>">
        <!---左侧-->
        <div class="w800 fl">
            <!--简历头部-->
            <div class="myresume-top inwrapBox mb20">
                <!-- <div class="myresume-top-tab">
                     <span class="cur tab-1">在线中文简历</span>
                     <span>|</span>
                     <span class="tab-2">English Resume Online</span>
                     <span>|</span>
                     <span class="tab-3">附件简历</span>
                 </div>-->
                <!--切换一-->
                <div class="tab-content-1">
                    <div class="resume-integrity-area clearfix">
                        <span class="fl">在线中文简历:</span>
                        <div class="resume-integrity fl">
                            <span class="integrity-solid transition_1000"></span>
                        </div>
                        <div class="resume-progress fl">简历完整度: <em class="integrity-number"></em></div>
                        <div class="fr">
                            <a class="btn-view" href="/resume/detail/<%=resume[i].rid%>" target="_blank">预览简历</a>
                            <span>|</span>
                            <span class="btn-downResume">下载</span>
                            <span>|</span>
                            <a class="find-jobs" href="/j/search" target="_blank">找实习</a>
                        </div>
                    </div>
                </div>
                <!--&lt;!&ndash;切换二&ndash;&gt;
                <div class="tab-content-2">

                </div>
                &lt;!&ndash;切换三&ndash;&gt;
                <div class="tab-content-3">

                </div>-->
            </div>

            <!--简历主体-->
            <div class="myresume-main inwrapBox mb20">
                <div class="myresume-main-top"></div>
                <!--基本信息 Start-->
                <div class="resume-header pd resume-block resume-block-base">
                    <% if(resume[i].avatar){
                        integrity+=10;
                    %>
                    <div class="avatar" style="background-image:url('<%=resume[i].avatar%>?imageView2/2/w/244/h/244')">
                        <div class="avatar-mask"></div>
                        <input type="file" id="avatar-file" data-uid = "<%=uid%>" name="file" class="avatar-file" data-rid="<%=resume[i].rid%>" title="支持jpg、jpeg、gif、png格式，文件小于5M"/>
                    </div>
                    <%}else{%>
                    <div class="avatar">
                        <div class="avatar-mask"></div>
                        <input type="file" id="avatar-file" data-uid = "<%=uid%>" name="file" class="avatar-file" data-rid="<%=resume[i].rid%>" title="支持jpg、jpeg、gif、png格式，文件小于5M"/>
                    </div>
                    <%}%>

                    <div class="info show-state">
                        <span class="myname name"> <%=resume[i].name%></span>
                    </div>
                    <div class="myresume-info">
                        <div class="resume-r">
                            <span class="btn-edit btn icon-r-edit"></span>
                        </div>
                        <ul class="resume-base clearfix">
                            <li>籍贯:<span class="address" title="<%=resume[i].address.length>12?resume[i].address:""%>"><%=resume[i].address.length>12?resume[i].address.substr(0,12)+"...":resume[i].address%></span></li>
                            <li>生日:<span class="birthday"><%=new Date(resume[i].birthday).format("yyyy.MM")%></span></li>
                            <li>性别:<%
                                    var sex = resume[i].male?"男":"女";
                                %><span class="sex" data-type="<%=resume[i].male%>"><%=sex%></span></li>
                            <li>电话:<span class="phone"><%=resume[i].phone%></span></li>
                            <li>邮箱:<span class="email"><%=resume[i].email%></span></li>
                        </ul>
                    </div>
                </div>
                <!--基本信息 End-->
                <!--基本信息 编辑模式 Start-->
                <div class="resume-edit resume-edit-base">
                    <div class="resume-edit-box">
                        <div class="resume-edit-item clearfix">
                            <span class="title fl">姓名</span>
                            <input type="text" class="name-edit fl req" value="<%=resume[i].name%>"/>
                            <div class="sex-select fl">
                                <%if(sex){%>
                                <span class="tab on" data-type="1">男</span>
                                <span class="tab" data-type="0">女</span>
                                <%}else{%>
                                <span class="tab" data-type="1">男</span>
                                <span class="tab on" data-type="0">女</span>
                                <%}%>
                            </div>
                        </div>
                        <div class="resume-edit-item clearfix">
                            <span class="title fl">籍贯</span>
                            <div class="selector-input fl address-wrap"><input type="text" class="address" value="<%=resume[i].address%>" readonly/></div>
                            <div class="selector-city fl"></div>
                            <span class="title tit-r fl">生日</span>
                            <span class="birth-wrap fl"><input type="text" class="birthday" id="birthday" readonly value="<%=new Date(resume[i].birthday).format("yyyy.MM")%>" /></span>
                        </div>
                        <div class="resume-edit-item clearfix">
                            <span class="title fl">手机号码</span>
                            <input type="text" class="phone fl" value="<%=resume[i].phone%>"/>
                            <span class="title tit-r fl">电子邮箱</span>
                            <input type="text" class="email" value="<%=resume[i].email%>"/>
                        </div>
                        <div class="btn-area">
                            <input type="button" class="confirm btn" value="确定"/>
                            <input type="button" class="console" value="取消"/>
                        </div>
                    </div>
                </div>
                <!--基本信息 编辑模式 End-->

                <!--教育信息 Start-->
                <div class="resume-block resume-block-edu pd">
                    <div class="resume-block-title">
                        <span class="icon icon-education"></span>教育背景
                        <div class="add-area">
                            <span class="btn-add btn"><span class="icon-r-add"></span>添加</span>
                        </div>
                    </div>
                    <%try{
                        var edu_list = JSON.parse(resume[i].education_detail);
                    if(edu_list.length>0){
                        integrity+=10;
                    %>
                    <%for(var j = 0;j<edu_list.length;j++){%>
                    <div class="resume-edu show-state" data-edu_id="<%=edu_list[j].edu_id%>">
                        <div class="info clearfix">
                            <span class="resume-circle"></span>
                            <span class="time fl"><%=edu_list[j].start_time.replace("-",".").substr(0,7)%>~<%=edu_list[j].end_time.replace("-",".").substr(0,7)%></span>
                            <span class="school fl"><%=edu_list[j].school%></span>
                        </div>
                        <div class="resume-r">
                            <%if(edu_list.length >1){%>
                            <span class="btn-edit btn" data-startime="<%=edu_list[j].start_time.replace("-",".").substr(0,7)%>" data-endtime="<%=edu_list[j].end_time.replace("-",".").substr(0,7)%>" data-school="<%=edu_list[j].school%>" data-major="<%=edu_list[j].major%>" data-stage="<%=edu_list[j].stage%>"><span class="icon-r-edit"></span></span>
                            <span class="btn-delete btn"><span class="icon-r-del"></span></span>
                            <%}else{%>
                            <span class="btn-edit btn" data-startime="<%=edu_list[j].start_time.replace("-",".").substr(0,7)%>" data-endtime="<%=edu_list[j].end_time.replace("-",".").substr(0,7)%>" data-school="<%=edu_list[j].school%>" data-major="<%=edu_list[j].major%>" data-stage="<%=edu_list[j].stage%>"><span class="icon-r-edit"></span></span>
                            <%}%>
                        </div>
                        <!--确认删除-->
                        <!--<div class="delete-box">-->
                        <!--<div class="delete-box-main">-->
                        <!--<p>确认删除本条消息吗?</p>-->
                        <!--<div class="delete-box-btn">-->
                        <!--<span class="btn btn-confirm-del">确定</span>-->
                        <!--<span class="btn btn-cancel">取消</span>-->
                        <!--</div>-->
                        <!--<span class="triangleBottom"></span>-->
                        <!--<span class="triangleBottom2"></span>-->
                        <!--</div>-->
                        <!--</div>-->
                        <div class="resume-edu-info">
                            <span class="major"><%=edu_list[j].major%></span><span class="stage"><%=edu_list[j].stage%></span>
                        </div>
                    </div>
                    <%} }
                    }catch(e){}%>
                </div>
                <!--教育信息 End-->
                <!--编辑模式 教育背景 Start-->
                <div class="resume-edit resume-edit-edu">
                    <div class="resume-edit-box">
                        <div class="resume-edit-item clearfix">
                            <span class="title fl">学校名称</span>
                            <span class="fl"><input class="edit-school" type="text" placeholder="请输入你的学校名称"/></span>
                        </div>
                        <div class="resume-edit-item clearfix">
                            <span class="title fl">专业名称</span>
                            <span class="fl"><input class="edit-major" type="text" placeholder="请输入你的专业名称"/></span>
                        </div>
                        <div class="resume-edit-item clearfix">
                            <span class="title fl">学历</span>
                            <div class="fl">
                                <span class="btn-selector"><input class="edit-stage" type="text" readonly/></span>
                                <ul class="selector fl">
                                    <li data-type="1">大专</li>
                                    <li data-type="2">本科</li>
                                    <li data-type="3">硕士</li>
                                    <li data-type="5">博士及以上</li>
                                    <li data-type="6">其他</li>
                                </ul>
                            </div>
                        </div>
                        <div class="resume-edit-item clearfix">
                            <div class="title fl">起止日期</div>
                            <div class="fl"><span><input class="edit-startTime" id="edu-start" readonly type="text" placeholder="开始日期" /></span> ~ <span><input class="edit-endTime" id="edu-end" readonly type="text" placeholder="结束日期" /></span></div>
                        </div>
                        <div class="btn-area">
                            <input type="button" class="confirm btn" value="确定"/>
                            <input type="button" class="console" value="取消"/>
                        </div>
                    </div>
                </div>
                <!--编辑模式 教育背景 end-->

                <!--期望实习 Start-->
                <div class="resume-block resume-block-hope pd">
                    <div class="resume-block-title">
                        <span class="icon icon-expecte-practice"></span>期望实习
                        <div class="resume-r">
                            <sapn class="btn-edit btn icon-r-edit"></sapn>
                        </div>
                    </div>
                    <%try{
                        integrity+=10;

                        var type_id = resume[i].intern_expect_position_type.split(",");
                        var type_text = [];
                        for(var j=0,len=type_id.length;j<len;j++){
                            var pid = parseInt(type_id[j].substr(0,1))-1;
                            var sid = parseInt(type_id[j].substr(1)) ;
                            if(pid>-1 && sid > -1){
                                type_text.push(job_type[pid].sub_types[sid].group_name);
                            }
                        }
                        var jt = type_text.toString();

                        var payment = "";
                        var payment_text = ["不限","50以下","50-100","100-200","200-500","500以上"];
                        switch(parseInt(resume[i].intern_expect_min_payment)){
                            case 0 :  payment = payment_text[0];break;
                            case 1 :  payment = payment_text[1];break;
                            case 50 :  payment = payment_text[2];break;
                            case 100 :  payment = payment_text[3];break;
                            case 200:  payment = payment_text[4];break;
                            case 500:  payment = payment_text[5];break;
                            default : payment = payment_text[0];break;
                        }

                        var days_type = parseInt((resume[i].intern_expect_days_type));
                        var days_text = ["1-2天","3天","4天","5天","6-7天"];
                        var days = days_text[days_type-1];

                        var dur_type = parseInt((resume[i].intern_expect_dur_type));
                        var dur_text = ["1个月以下","2个月","3个月","3个月以上"];
                        var duration = dur_text[dur_type-1];

                    %>
                    <%}catch(e){}%>
                    <ul class="resume-hope clearfix show-state">
                        <li><span class="icon icon-new"></span><span class="hope-position" title="<%=resume[i].intern_expect_position.length>10?resume[i].intern_expect_position:""%>"><%=resume[i].intern_expect_position.length>10?resume[i].intern_expect_position.substr(0,10):resume[i].intern_expect_position%></span></li>
                        <li><span class="icon icon-job-type"></span><span class="hope-position-type" data-id="<%=resume[i].intern_expect_position_type%>"><%=jt%></span></li>
                        <li><span class="icon icon-locate"></span><span class="hope-city" data-cid="<%=resume[i].intern_expect_cid||0%>"><%=resume[i].intern_expect_city%></span></li>
                        <li><span class="icon icon-salary"></span><span class="hope-payment" data-type="<%=resume[i].intern_expect_min_payment%>"><%=payment%></span></li>
                        <li><span class="icon icon-calendar"></span><span class="hope-days" data-type="<%=resume[i].intern_expect_days_type%>"><%=days%>/周</span></li>
                        <li><span class="icon icon-r-time"></span><span class="hope-duration" data-type="<%=resume[i].intern_expect_dur_type%>"><%=duration%></span></li>
                    </ul>
                </div>
                <!--期望实习 End-->
                <!--编辑模式 期望实习 Start-->
                <div class="resume-edit resume-edit-hope">
                    <div class="resume-edit-box">
                        <div class="resume-edit-item clearfix">
                            <span class="title fl">实习城市</span>
                            <div class="fl">
                                <span class="selector-input hope-city-wrap"><input class="edit-city" value="" type="text" readonly/></span>
                                <span class="selector-city" style="margin-left: 0"></span>
                            </div>
                            <span class="title tit-r fl">期待日薪</span>
                            <span class="btn-selector fl"><input class="edit-payment" readonly type="text" data-type="<%=resume[i].intern_expect_min_payment || 0%>"/></span>
                            <ul class="selector selector-l" style="left:422px; top: 34px;">
                                <li data-type="0">不限</li>
                                <li data-type="1">50以下</li>
                                <li data-type="50">50-100</li>
                                <li data-type="100">100-200</li>
                                <li data-type="200">200-500</li>
                                <li data-type="500">500以上</li>
                            </ul>
                        </div>
                        <div class="resume-edit-item clearfix">
                            <span class="title fl">职位名称</span>
                            <span class="btn-selector fl"><input class="edit-position" type="text" value="<%=resume[i].intern_expect_position%>"/></span>
                        </div>
                        <div class="resume-edit-item clearfix">
                            <span class="title fl">职位类别</span>
                            <div class="fl"><input class="edit-position-type" readonly type="text" data-id="<%=resume[i].intern_expect_position_type || 0%>"/></div>
                        </div>
                        <div class="resume-edit-item clearfix">
                            <span class="title fl">每周实习</span>
                        <span class="edit-days fl">
                            <span class="tab" data-type="1">1-2天</span>
                            <span class="tab" data-type="2">3天</span>
                            <span class="tab" data-type="3">4天</span>
                            <span class="tab" data-type="4">5天</span>
                            <span class="tab" data-type="5">6-7天</span>
                        </span>
                        </div>
                        <div class="resume-edit-item clearfix">
                            <span class="title fl">连续实习</span>
                        <span class="edit-duration fl">
                            <span class="tab" data-type="1">1个月以下</span>
                            <span class="tab" data-type="2">2个月</span>
                            <span class="tab" data-type="3">3个月</span>
                            <span class="tab" data-type="4">3个月以上</span>
                        </span>
                        </div>
                        <div class="btn-area">
                            <input type="button" class="confirm btn" value="确定" data-rid="<%=resume[i].rid%>"/>
                            <input type="button" class="console" value="取消"/>
                        </div>
                    </div>
                </div>
                <!--期望实习 编辑模式 end-->

                <!--校园经历 Start-->
                <div class="resume-block resume-block-school pd">
                    <div class="resume-block-title">
                        <span class="icon icon-school-exper"></span>校园经历
                        <div class="add-area">
                            <span class="btn-add btn"><span class="icon-r-add"></span>添加</span>
                        </div>
                    </div>
                    <%
                    try{
                        var exp_list = JSON.parse(resume[i].school_exp);
                    if(exp_list.length>0){
                        integrity+=10;
                    for(var j = 0;j < exp_list.length;j++){
                    %>
                    <div class="resume-school resume-exp show-state"  data-id="<%=exp_list[j].id%>">
                        <div class="info clearfix">
                            <span class="resume-circle"></span>
                            <span class="time fl"><%=exp_list[j].startTime.replace("-",".").substr(0,7)%>~<%=exp_list[j].endTime.replace("-",".").substr(0,7)%></span>
                            <span class="exp-name fl"><%=exp_list[j].name%></span>
                            <span class="exp-role fl"><%=exp_list[j].role%></span>
                        </div>
                        <div class="resume-r">
                            <span class="btn-edit btn" data-rid="<%=resume[i].rid%>" data-id="<%=exp_list[j].id%>" data-startime="<%=exp_list[j].startTime.replace("-",".").substr(0,7)%>" data-endtime="<%=exp_list[j].endTime.replace("-",".").substr(0,7)%>" data-name="<%=exp_list[j].name%>" data-role="<%=exp_list[j].role%>" data-content="<%=exp_list[j].content%>"><span class="icon-r-edit"></span></span>
                            <span class="btn-delete btn"><span class="icon-r-del"></span></span>
                        </div>
                        <div class="exp-content">
                            <%-exp_list[j].content%>
                        </div>
                    </div>
                    <% } }
                    }catch(e){}%>
                </div>
                <!--校园经历 End-->
                <!--编辑模式 校园经历 Start-->
                <div class="resume-edit resume-edit-school">
                    <div class="resume-edit-box">
                        <div class="resume-edit-item clearfix">
                            <span class="title fl">校园经历</span>
                            <span class="fl"><input class="edit-name" type="text" placeholder="请输入你的校园经历名称"/></span>
                        </div>
                        <div class="resume-edit-item clearfix">
                            <div class="title fl">起止日期</div>
                            <div class="fl"><span><input class="edit-startTime" id="school-start" readonly type="text" placeholder="开始日期"/></span> - <span><input class="edit-endTime" id="school-end" readonly type="text" placeholder="结束日期"/></span></div>
                        </div>
                        <div class="resume-edit-item clearfix">
                            <span class="title fl">校园角色</span>
                            <span class="fl"><input class="edit-role" type="text" placeholder="如外联负责人"/></span>
                        </div>
                        <div class="resume-edit-item clearfix">
                            <span class="title fl">经历内容</span>
                            <span class="fl editor-content"><textarea id="content-school" class="edit-content" placeholder="请大致描述你的校园经历或做过的事儿"></textarea></span>
                        </div>
                        <div class="btn-area">
                            <input type="button" class="confirm btn" value="确定"/>
                            <input type="button" class="console" value="取消"/>
                        </div>
                    </div>
                </div>
                <!--编辑模式 校园经历 End-->

                <!--实习经历 Start-->
                <div class="resume-block resume-block-intern pd">
                    <div class="resume-block-title">
                        <span class="icon icon-internship"></span>实习经历
                        <div class="add-area">
                            <span class="btn-add btn"><span class="icon-r-add"></span>添加</span>
                        </div>
                    </div>
                    <%
                    try{
                        var exp_list = JSON.parse(resume[i].inter_exp);
                    if(exp_list.length>0){
                        integrity+=10;
                    for(var j = 0;j < exp_list.length;j++){
                    %>
                    <div class="resume-intern resume-exp show-state"  data-id="<%=exp_list[j].id%>">
                        <div class="info clearfix">
                            <span class="resume-circle"></span>
                            <span class="time fl"><%=exp_list[j].startTime.replace("-",".").substr(0,7)%>~<%=exp_list[j].endTime.replace("-",".").substr(0,7)%></span>
                            <span class="exp-name fl"><%=exp_list[j].name%></span>
                            <span class="exp-role fl"><%=exp_list[j].role%></span>
                        </div>
                        <div class="resume-r">
                            <span class="btn-edit btn" data-rid="<%=resume[i].rid%>" data-id="<%=exp_list[j].id%>" data-startime="<%=exp_list[j].startTime.replace("-",".").substr(0,7)%>" data-endtime="<%=exp_list[j].endTime.replace("-",".").substr(0,7)%>" data-name="<%=exp_list[j].name%>" data-role="<%=exp_list[j].role%>" data-content="<%=exp_list[j].content%>"><span class="icon-r-edit"></span></span>
                            <span class="btn-delete btn"><span class="icon-r-del"></span></span>
                        </div>
                        <div class="exp-content">
                            <%-exp_list[j].content%>
                        </div>
                    </div>
                    <% } }
                    }catch(e){}%>
                </div>
                <!--实习经历 End-->
                <!--编辑模式 实习经历 Start-->
                <div class="resume-edit resume-edit-intern">
                    <div class="resume-edit-box">
                        <div class="resume-edit-item clearfix">
                            <span class="title fl">实习公司</span>
                            <span class="fl"><input class="edit-name"  type="text"placeholder="请输入你的实习公司名称"/></span>
                        </div>
                        <div class="resume-edit-item clearfix">
                            <div class="title fl">实习时间</div>
